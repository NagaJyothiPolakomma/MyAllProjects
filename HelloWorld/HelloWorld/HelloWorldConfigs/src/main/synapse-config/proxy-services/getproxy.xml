<?xml version="1.0" encoding="UTF-8"?>
<proxy name="getproxy" startOnLoad="true" transports="http https" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <inSequence>
            <log level="custom">
                <property expression="$ctx:proxy.name" name="proxy service name "/>
            </log>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </target>
</proxy>
